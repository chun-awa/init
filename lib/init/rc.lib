#!/bin/sh
RESET=$(printf "\033[0m")
RED=$(printf "\033[91m")
YELLOW=$(printf "\033[93m")
GREEN=$(printf "\033[92m")
CYAN=$(printf "\033[96m")
BLUE=$(printf "\033[94m")
MAGENTA=$(printf "\033[95m")
WHITE=$(printf "\033[97m")
log(){
    echo "${GREEN}*${RESET} ${WHITE}${1}${RESET}"
}
log_info(){
    echo "${CYAN}*${RESET} ${WHITE}${1}${RESET}"
}
log_error(){
    echo "${RED}*${RESET} ${WHITE}${1}${RESET}"
}

